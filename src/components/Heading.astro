---
interface Props {
  title: string;
}

const { title }: Props = Astro.props;
---

<section class="bg-tertiary py-14">
  <div class="container flex flex-col gap-6">
    <h1 class="text-primary text-4xl font-bold">{title}</h1>
    <hr class="bg-white border-0 rounded w-9 h-2" />
  </div>
</section>
